<!doctype html>
<html>
	<head>
		<title>{{title}} - MyPage</title>
		<link rel="stylesheet" href="/assets/style.css">
	</head>
<body>
	{{> header test=true}}

	<main>
		<section class="band -double container">
			<ul class="articles-list">
				{{#each collections.posts}}
				<li class="articles-list__item">
					{{> article articleData=this }}
				</li>
				{{/each}}
			</ul>
		</section>
	</main>
</body>
</html>
